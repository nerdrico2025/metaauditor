{
  "name": "click-auditor",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "workspaces": [
    "server",
    "client",
    "landing"
  ],
  "scripts": {
    "dev": "npm run dev --workspaces --if-present",
